<li class="event [[+eo:ne=`0`:then=`alt`]]">
    <a href="[[+infoURL]]" rel="nofollow" class="gccalevent" data-index="[[+id]]">
        <div class="date">[[+start:date=`<span>%b</span><br/>%e`]]</div>
        <div class="details"><strong>[[+title:ellipsis=`30`]]</strong><br />[[+span]] &bull; [[+ad:ne=`1`:then=`[[+start:date=`%l:%M %P`]]`:else=`All Day`]]</div>
        <div class="expand fa fa-expand"></div>
    </a>
</li>